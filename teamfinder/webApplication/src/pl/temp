groupManager.getGroupById(groupIds[i], function (error, group) {
                                                    if (error) {
                                                        const model = {
                                                            error,
                                                            csrfToken: request.csrfToken()
                                                        }
                                                        response.render('group-finder.hbs', model)
                                                    }
                                                    else {
                                                        groups.splice(group.GroupId, 1) // pop specific element
                                                        console.log(groups)
                                                    }
                                                })